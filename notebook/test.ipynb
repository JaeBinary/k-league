   "source": [
    "import pandas as pd\n",
    "from pandasql import sqldf\n",
    "\n",
    "# CSV 파일 읽기\n",
    "df = pd.read_csv(r'..\\data\\match\\kleague_match_info_2025.csv')\n",
    "\n",
    "# SQL 쿼리 직접 사용\n",
    "query = \"\"\"\n",
    "SELECT DISTINCT weather\n",
    "FROM df\n",
    "\"\"\"\n",
    "\n",
    "result = sqldf(query, locals())\n",
    "\n",
    "display(result.dtypes.to_frame(name='dtype'))\n",
    "\n",
    "result"
   ]
  }
